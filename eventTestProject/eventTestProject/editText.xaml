<Window x:Class="eventTestProject.editText"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:eventTestProject"
        mc:Ignorable="d"
        Title="editText" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="textBoxEditText" HorizontalAlignment="Left" Height="103" Margin="10,18,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="274"/>
        <Button x:Name="button" Content="Add &amp; Save" HorizontalAlignment="Left" Margin="19,162,0,0" VerticalAlignment="Top" Width="75" IsDefault="True" Click="button_Click"/>
        <Button x:Name="button1" Content="Cancel" HorizontalAlignment="Left" Margin="199,232,0,0" VerticalAlignment="Top" Width="75" IsCancel="True" Click="button1_Click"/>
        <Button x:Name="button2" Content="Add" HorizontalAlignment="Left" Margin="199,162,0,0" VerticalAlignment="Top" Width="75" Click="button2_Click"/>

    </Grid>
</Window>
